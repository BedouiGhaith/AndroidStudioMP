<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearNotification"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnarrowleft"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowbroadcomcorp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.72"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtNotification"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_21pxh"
                            android:layout_weight="0.86"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@string/lbl_notification"
                            style="@style/txtOverpassbold16" tools:text="@string/lbl_notification"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBroadcomCorp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_158pxh"
                        android:layout_weight="0.28"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_clear_all"
                        style="@style/txtOverpassromanregular13_1" tools:text="@string/lbl_clear_all"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <FrameLayout android:id="@+id/frameStackbookmark"
                android:layout_height="@dimen/_94pxv"
                android:layout_marginTop="@dimen/_20pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearColumnbookmark"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A701">
                    <LinearLayout android:id="@+id/linearRowbookmark"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageBookmark"
                            android:layout_height="@dimen/_22pxh"
                            android:layout_marginBottom="@dimen/_29pxv"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_bookmark"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_bookmark"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnweknowthat"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_weight="0.95"
                            android:layout_width="0dp" android:orientation="vertical">
                            <LinearLayout
                                android:id="@+id/linearRowweknowthat"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/txtWeknowthat"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.97"
                                    android:layout_width="0dp"
                                    android:lineSpacingExtra="@dimen/_3pxh"
                                    android:text="@string/msg_we_know_that"
                                    style="@style/txtOverpassromanregular14" tools:text="@string/msg_we_know_that"/>
                                <View
                                    android:background="@drawable/rectangle_bg_red_400_radius_4"
                                    android:id="@+id/viewEllipse406"
                                    android:layout_height="@dimen/_8pxh"
                                    android:layout_marginBottom="@dimen/_25pxv"
                                    android:layout_marginStart="@dimen/_27pxh"
                                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_8pxh"/>
                            </LinearLayout>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTime"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@string/msg_aug_12_2022_at"
                                style="@style/txtOverpassregular13_1" tools:text="@string/msg_aug_12_2022_at"/>
                        </LinearLayout>
                    </LinearLayout>
                    <View android:background="@color/gray_202"
                        android:id="@+id/linePathTwo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_2pxv"
                        android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_375pxh"/>
                </LinearLayout>
                <View android:background="@color/gray_202"
                    android:id="@+id/linePathThree"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_2pxv"
                    android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_375pxh"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowcalendar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCalendar"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_calendar"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
                <TextView android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_weight="0.93"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:text="@string/msg_the_future_of_p"
                    style="@style/txtOverpassregular14_3" tools:text="@string/msg_the_future_of_p"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTimeOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_56pxh"
                android:layout_marginStart="@dimen/_56pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/msg_aug_12_2022_at"
                style="@style/txtOverpassregular13_1" tools:text="@string/msg_aug_12_2022_at"/>
        </LinearLayout>
        <View android:background="@color/gray_202"
            android:id="@+id/linePath2CopyTwo"
            android:layout_height="@dimen/_2pxv"
            android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_375pxh"/>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerNotification"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_282pxv"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_37pxh}" tools:itemCount="3" tools:listitem="@layout/row_notification"/>
    </LinearLayout>
</layout>

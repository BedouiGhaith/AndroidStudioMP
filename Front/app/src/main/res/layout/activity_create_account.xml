<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
        <LinearLayout android:gravity="start"
        android:id="@+id/linearCreateaccount"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageArrowleft"
            android:layout_height="@dimen/_14pxh"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="@dimen/_14pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_arrowleft"
            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtCreateyouracc"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@string/msg_create_your_acc"
            style="@style/txtOverpassbold24" tools:text="@string/msg_create_your_acc"/>
        <TextView android:ellipsize="end" android:id="@+id/txtUsername"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_38pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/lbl_your_name"
            style="@style/txtOverpassregular15" tools:text="@string/lbl_your_name"/>
        <View android:background="@color/bluegray_900_19"
            android:id="@+id/username_txt"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_22pxv" android:layout_width="@dimen/_311pxh"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtUsernameOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/lbl_mobile_number"
            style="@style/txtOverpassromanregular15" tools:text="@string/lbl_mobile_number"/>
        <View android:background="@color/bluegray_900_19"
            android:id="@+id/password_txt"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_22pxv" android:layout_width="@dimen/_311pxh"/>
        <TextView android:ellipsize="end" android:id="@+id/txtPassword"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="wrap_content"
            android:singleLine="true" android:text="@string/lbl_email"
            style="@style/txtOverpassromanregular15" tools:text="@string/lbl_email"/>
        <View android:background="@color/bluegray_900_19"
            android:id="@+id/lineLineOneTwo"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_311pxh"/>
        <LinearLayout android:id="@+id/linearRowpasswordOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtPasswordOne"
                android:layout_height="wrap_content"
                android:layout_weight="0.79" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_password"
                style="@style/txtOverpassromanregular15" tools:text="@string/lbl_password"/>
            <FrameLayout android:id="@+id/frameStackstroke296"
                android:layout_height="@dimen/_10pxv"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_233pxh"
                android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_16pxh">
                <ImageView android:id="@+id/imageStroke296"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_5pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_4pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_stroke296"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_stroke296"/>
                <ImageView android:id="@+id/imageEye"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_eye"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
            </FrameLayout>
        </LinearLayout>
        <View android:background="@color/bluegray_900_19"
            android:id="@+id/lineLineOneThree"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_311pxh"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnCreateAccount"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_17pxv" android:singleLine="true"
            android:text="@string/lbl_create_account"
            android:textAllCaps="true" style="@style/btnSolidRounded" tools:text="@string/lbl_create_account"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowarrowleftOne"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_301pxv"
            android:layout_marginEnd="@dimen/_91pxh"
            android:layout_marginStart="@dimen/_91pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageArrowleftOne"
                android:layout_height="@dimen/_8pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_4pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft_bluegray_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_bluegray_900"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_weight="0.97" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/msg_already_have_ac"
                style="@style/txtAbelregular14" tools:text="@string/msg_already_have_ac"/>
        </LinearLayout>
    </LinearLayout>
</layout>

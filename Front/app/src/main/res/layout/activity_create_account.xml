<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:id="@+id/linearCreateaccount"
        style="@style/groupStylewhite_A700"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearRowarrowleftOne"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_18pxv"
            android:gravity="start"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageArrowleft"
                android:layout_width="@dimen/_14pxh"
                android:layout_height="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginEnd="@dimen/_29pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_arrowleft" />
        </LinearLayout>

        <TextView
            android:id="@+id/txtCreateyouracc"
            style="@style/txtOverpassbold24"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_marginEnd="@dimen/_32pxh"
            android:ellipsize="end"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@string/msg_create_your_acc"
            tools:text="@string/msg_create_your_acc" />

        <TextView
            android:id="@+id/txtUsername"
            style="@style/txtOverpassregular15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_20sp"
            android:layout_marginEnd="@dimen/_29pxh"
            android:ellipsize="end"
            android:singleLine="true"
            tools:text="Username" />

        <EditText
            android:id="@+id/r_username"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:background="@color/bluegray_900_19"
            android:textSize="22sp" />

        <LinearLayout
            android:id="@+id/linearRowpasswordOne"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txtPasswordOne"
                style="@style/txtOverpassromanregular15"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.79"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_password"
                tools:text="@string/lbl_password" />

            <FrameLayout
                android:id="@+id/frameStackstroke296"
                android:layout_width="@dimen/_16pxh"
                android:layout_height="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_233pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_marginBottom="@dimen/_1pxv">

                <ImageView
                    android:id="@+id/imageStroke296"
                    android:layout_width="@dimen/_4pxh"
                    android:layout_height="@dimen/_5pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_stroke296"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_stroke296" />

                <ImageView
                    android:id="@+id/imageEye"
                    android:layout_width="@dimen/_16pxh"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_gravity="start"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_eye"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_eye" />
            </FrameLayout>
        </LinearLayout>

        <EditText
            android:id="@+id/r_password_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:background="@color/bluegray_900_19"
            android:textSize="22sp" />

        <EditText
            android:id="@+id/r_confirmPass"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:background="@color/bluegray_900_19"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/txtPassword"
            style="@style/txtOverpassromanregular15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_marginEnd="@dimen/_32pxh"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@string/lbl_email"
            tools:text="@string/lbl_email" />

        <EditText
            android:id="@+id/r_email"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:background="@color/bluegray_900_19"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/txtUsernameOne"
            style="@style/txtOverpassromanregular15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@string/lbl_mobile_number"
            tools:text="@string/lbl_mobile_number" />

        <EditText
            android:id="@+id/r_mobile"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:background="@color/bluegray_900_19"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/txtPassword2"
            style="@style/txtOverpassromanregular15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_marginEnd="@dimen/_32pxh"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="Address"
            tools:text="Address" />

        <EditText
            android:id="@+id/r_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:background="@color/bluegray_900_19"
            android:textSize="22sp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnCreateAccount"
            style="@style/btnSolidRounded"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:ellipsize="end"
            android:gravity="center"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_17pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingBottom="@dimen/_17pxv"
            android:singleLine="true"
            android:text="@string/lbl_create_account"
            android:textAllCaps="true"
            tools:text="@string/lbl_create_account" />

        <TextView
            android:id="@+id/txtLanguage"
            style="@style/txtAbelregular14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:singleLine="true"
            android:text="@string/msg_already_have_ac"
            tools:text="@string/msg_already_have_ac" />

    </LinearLayout>
</layout>

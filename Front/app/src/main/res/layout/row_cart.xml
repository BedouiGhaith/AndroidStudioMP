<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <LinearLayout android:id="@+id/linearRowmentos1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageImage134"
            android:layout_height="@dimen/_80pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="@dimen/_70pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_6pxh}"
            imageRes="@{@drawable/img_image134_80x70}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image134_80x70"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnmentos"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_weight="0.77" android:layout_width="0dp" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowmentos2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMentos"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_weight="0.7"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_mentos"
                    style="@style/txtOverpasssemibold14" tools:text="@string/lbl_mentos"/>
                <ImageView android:id="@+id/imageClose"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_169pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtBottleof50pe"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/msg_bottle_of_50_pe"
                style="@style/txtOverpassregular13_2" tools:text="@string/msg_bottle_of_50_pe"/>
            <LinearLayout android:id="@+id/linearRowprice"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.26"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_25"
                    style="@style/txtOverpassbold18_1" tools:text="@string/lbl_25"/>
                <LinearLayout android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_103pxh"
                    android:layout_weight="0.74"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStylegray_51cornerRadius">
                    <FrameLayout android:id="@+id/frameStackvector"
                        android:layout_height="@dimen/_32pxv"
                        android:layout_width="@dimen/_32pxh" style="@style/groupStylegreen_50cornerRadius">
                        <ImageView android:id="@+id/imageVector"
                            android:layout_gravity="bottom|center"
                            android:layout_height="@dimen/_1pxh"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="@dimen/_12pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_green_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_green_a700"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_weight="0.12"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_1"
                        style="@style/txtAbelregular16" tools:text="@string/lbl_1"/>
                    <FrameLayout android:id="@+id/frameStackplus"
                        android:layout_height="@dimen/_32pxv"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_width="@dimen/_32pxh" style="@style/groupStylegreen_A200cornerRadius">
                        <ImageView android:id="@+id/imagePlusOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_marginHorizontal="@dimen/_10pxh"
                            android:layout_marginVertical="@dimen/_10pxv"
                            android:layout_width="@dimen/_12pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_plus_1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_plus_1"/>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>

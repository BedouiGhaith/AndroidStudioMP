<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <LinearLayout android:gravity="end"
        android:id="@+id/linearColumnmentos"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical" style="@style/groupStylewhite_6">
        <FrameLayout android:id="@+id/frameStack"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_158pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="@dimen/_155pxh" style="@style/groupStylegray_102cornerRadius">
            <ImageView android:id="@+id/imageImageTwentyTwo"
                android:layout_gravity="center"
                android:layout_height="@dimen/_129pxh"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="@dimen/_127pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image22}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image22"/>
        </FrameLayout>
        <TextView android:ellipsize="end" android:id="@+id/cart_row_name"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_98pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true" android:text="@string/lbl_mentos"
            style="@style/txtOverpassregular13" tools:text="@string/lbl_mentos"/>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearRowdcounter"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtDCounterTwo"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_weight="0.34" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true" android:text="@string/lbl_2_d"
                style="@style/txtOverpasssemibold16" tools:text="@string/lbl_2_d"/>
            <LinearLayout android:id="@+id/linearRowstarTwo"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_67pxh"
                android:layout_weight="0.66" android:layout_width="0dp"
                android:orientation="horizontal" style="@style/groupStyleamber_A400cornerRadius">
                <ImageView android:id="@+id/imageStarTwo"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFortyTwoTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.61"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_4_2"
                    style="@style/txtOverpassbold13_1" tools:text="@string/lbl_4_2"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>

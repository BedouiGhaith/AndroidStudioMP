<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <LinearLayout android:gravity="end"
        android:id="@+id/linearColumnsignal"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical" style="@style/groupStylewhite_6">
        <FrameLayout android:id="@+id/frameStackOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_158pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_155pxh">
            <FrameLayout android:id="@+id/frameStack"
                android:layout_gravity="center"
                android:layout_height="@dimen/_158pxv"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_155pxh" style="@style/groupStylegray_102cornerRadius">
                <ImageView android:id="@+id/imageImageTwenty"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_119pxh"
                    android:layout_marginBottom="@dimen/_21pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="@dimen/_116pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle927}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle927"/>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStacksignal"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_57pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_57pxh">
                <ImageView android:id="@+id/imageSignal"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_57pxh"
                    android:layout_width="@dimen/_57pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_signal"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSALE"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_sale"
                    style="@style/txtOverpassbold12" tools:text="@string/lbl_sale"/>
            </FrameLayout>
        </FrameLayout>
        <TextView android:gravity="start"
            android:id="@+id/item_product_name"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_47pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_6pxh"
            android:text="@string/msg_panadol_500mg_1"
            style="@style/txtOverpassregular13" tools:text="@string/msg_panadol_500mg_1"/>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearRowdcounter"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/item_product_price"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_weight="0.4" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@string/lbl_10_d"
                style="@style/txtOverpasssemibold16" tools:text="@string/lbl_10_d"/>
            <LinearLayout android:id="@+id/linearRowstar"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_60pxh"
                android:layout_weight="0.6" android:layout_width="0dp"
                android:orientation="horizontal" style="@style/groupStyleamber_A400cornerRadius">
                <ImageView android:id="@+id/imageStar"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFortyTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.61"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_4_2"
                    style="@style/txtOverpassbold13_1" tools:text="@string/lbl_4_2"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>

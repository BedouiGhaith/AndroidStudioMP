<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".appcomponents.activities.FragmentContainerActivity">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentContainerView"
        android:name="com.androidproject.app.appcomponents.activities.ProductsFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/frameBottombar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/frameBottombar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragmentContainerView">

        <LinearLayout
            android:id="@+id/linearRowhome"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@drawable/space_medium"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:showDividers="middle">

            <ImageView
                android:id="@+id/imageHome"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                android:src="@drawable/img_home_16x14"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_home_16x14" />

            <ImageView
                android:id="@+id/imageNotification"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                android:src="@drawable/img_notification_19x16"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_notification_19x16" />

            <FrameLayout
                android:id="@+id/frameStackrectangle929"
                android:layout_width="@dimen/_40pxh"
                android:layout_height="@dimen/_40pxv">

                <View
                    android:id="@+id/viewRectangle929"
                    android:layout_width="@dimen/_40pxh"
                    android:layout_height="@dimen/_40pxh"
                    android:background="@drawable/rectangle_bg_green_600_radius_6" />

                <ImageView
                    android:id="@+id/imagePlus"
                    android:layout_width="@dimen/_18pxh"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_gravity="center"
                    android:layout_marginHorizontal="@dimen/_11pxh"
                    android:layout_marginVertical="@dimen/_11pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_plus"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_plus" />
            </FrameLayout>

            <ImageView
                android:id="@+id/imageBag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                android:src="@drawable/img_bag_16x14"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_bag_16x14" />

            <ImageView
                android:id="@+id/imageUser"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                android:src="@drawable/img_user_2"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_user_2" />
        </LinearLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>